{
  "version": "0.1.0",
  "require": {
    "google/cloud-functions-framework": "^1.0",
    "nfephp-org/sped-da": "^1.0"
  },
  "scripts": {
    "start": "$env:FUNCTION_TARGET='gerarDANFE' && php -S localhost:8080 vendor/google/cloud-functions-framework/router.php",
